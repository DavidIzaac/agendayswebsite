{
  "extends": "../tsconfig.json",
  "compilerOptions": {
    "outDir": "../out-tsc/app",
    "types": [],
    "baseUrl" : "./",
    "paths": {
      "@angular/*": [
        "node_modules/@angular/*"
      ],
      "@auth/*": [
        "src/app/auth/*"
      ],
      "@shared/*": [
        "src/app/shared/*"
      ],
      "@assets/*": [
        "src/assets/*"
      ]
    },
  },
  "exclude": [
    "test.ts",
    "**/*.spec.ts"
  ]
}
